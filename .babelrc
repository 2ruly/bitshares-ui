{
    "presets": [
        ["es2015", { "modules": false }],
        "stage-0",
        "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "lodash"
    ],
    "env": {
        "test": {
            "presets": ["es2015", "stage-0","react"],
            "plugins": [
                ["module-resolver", {
                    "root": ["./app", "./app/lib"],
                    "alias": {
                      "test": "./test",
                      "bitsharesjs/es": "bitsharesjs"
                    }
                  }],
                  ["transform-define", {
                      "__ELECTRON__": false,
                      "__DEV__": false,
                      "__HASH_HISTORY__": false
                      }]
              ]

        }
    }
}
